{
    "version": 2,
    "builds": [
        {
            "src": "api/subscribe.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/subscribe",
            "dest": "/api/subscribe.js"
        },
        {
            "src": "/a-venir",
            "dest": "/a-venir.html"
        },
        {
            "src": "/a-venir.html",
            "dest": "/a-venir.html"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ]
}